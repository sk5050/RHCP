#!/usr/bin/env python

import sys
from graph import Node, Graph


class LAOStar(object):

    def __init__(self, model, lb=None, ub=None, alpha=0.0):

        self.model = model
        self.lb = lb
        self.ub = ub
        self.alpha = alpha
        
        self.graph = Graph

        self.graph = Graph(name='G')
        self.graph.add_node(model.init_state, value=None)

        
